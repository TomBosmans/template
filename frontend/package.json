    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.17",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.2",
    "@ladle/react": "^5.1.1",
    "@tolgee/cli": "^2.14.0",
    "@types/node": "^24.10.1",
